model{
  # metab | T1D
  for (i in 1:N) {
    metab[i] ~ dnorm(alpha0 + alpha*t1d[i], tau)
    }
  # methyl | T1D, metab
  for (i in 1:N) {
    methyl[i] ~ dnorm(int + beta*t1d[i] + gamma*metab[i], tau)
    }
  
   # Very flat priors
   alpha0 ~ dnorm(0,0.0001)
   alpha ~ dnorm(0,0.0001)
   beta ~ dnorm(0,0.0001)
   int ~ dnorm(0,0.0001)
   gamma ~ dnorm(0,0.0001)

  tau ~ dgamma(0.0001, 0.0001)
  sigma2 <- 1/tau
  }
  