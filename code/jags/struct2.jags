model{
  # methyl | T1D
  for (i in 1:N) {
    methyl[i] ~ dnorm(beta0 + beta*t1d[i], tau)
    }
  # metab | T1D, methyl
  for (i in 1:N) {
    metab[i] ~ dnorm(int + alpha*t1d[i] + gamma*methyl[i], tau)
    }
  
  
   # Very flat priors
   alpha ~ dnorm(0,0.0001)
   beta0 ~ dnorm(0,0.0001)
   beta ~ dnorm(0,0.0001)
   int ~ dnorm(0,0.0001)
   gamma ~ dnorm(0,0.0001)

  tau ~ dgamma(0.0001, 0.0001)
  sigma2 <- 1/tau
  }
  